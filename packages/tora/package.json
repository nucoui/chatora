{
  "name": "tora",
  "version": "0.0.0",
  "private": false,
  "description": "Framework for creating custom elements with JSX function components",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./reactivity": {
      "types": "./dist/reactivity.d.ts",
      "import": "./dist/reactivity.mjs",
      "require": "./dist/reactivity.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.mjs",
      "require": "./dist/jsx-dev-runtime.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "@tora/reactivity": "workspace:*",
    "@tora/runtime": "workspace:*"
  },
  "devDependencies": {
    "@tora/config": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
