{
  "name": "@katu/transpiler",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "packageManager": "pnpm@10.8.1",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./JSX": {
      "types": "./types/JSX.namespace.d.ts"
    },
    "./DefineCustomElement": {
      "types": "./types/defineCustomElement.d.ts"
    }
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "test": "tsup && vitest"
  },
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0"
  },
  "devDependencies": {
    "@babel/types": "^7.27.0",
    "@katu/config": "workspace:*",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.15.3",
    "eslint": "catalog:",
    "tsup": "^8.4.0",
    "vitest": "^3.1.1"
  }
}
