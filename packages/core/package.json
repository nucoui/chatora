{
  "name": "chatora",
  "type": "module",
  "version": "0.0.1-beta.0",
  "private": false,
  "description": "Framework for creating custom elements with JSX function components",
  "author": "nuco <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "license": "MPL-2.0",
  "homepage": "https://github.com/nucoui/tora",
  "repository": {
    "type": "git",
    "url": "https://github.com/nucoui/tora"
  },
  "bugs": {
    "url": "https://github.com/nucoui/tora/issues",
    "email": "takuma-ru@takumaru.dev"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./reactivity": {
      "types": "./dist/reactivity.d.ts",
      "import": "./dist/reactivity.mjs",
      "require": "./dist/reactivity.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.mjs",
      "require": "./dist/jsx-dev-runtime.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "release": "rlse",
    "release:test": "rlse -l patch -k config,commit-changes,create-release-branch --dry-run"
  },
  "dependencies": {
    "@tora/reactivity": "workspace:*",
    "@tora/runtime": "workspace:*"
  },
  "devDependencies": {
    "@takuma-ru/rlse": "0.0.1-beta.5",
    "@tora/config": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}