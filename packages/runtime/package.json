{
  "name": "@chatora/runtime",
  "version": "0.0.0",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./JSX": {
      "types": "./types/JSX.namespace.d.ts"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.mjs",
      "require": "./dist/jsx-dev-runtime.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "test": "tsup && vitest run --coverage"
  },
  "devDependencies": {
    "@chatora/config": "workspace:*",
    "@chatora/reactivity": "workspace:*",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "^3.1.1"
  }
}
