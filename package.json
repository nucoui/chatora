{
  "name": "catto",
  "version": "1.0.0",
  "scripts": {
    "p:core": "pnpm -C packages/core",
    "p:rtv": "pnpm -C packages/reactivity",
    "p:rt": "pnpm -C packages/runtime",
    "p:ts": "pnpm -C packages/transpiler",
    "pg:vanilla": "pnpm -C playgrounds/vanilla",
    "watch": "concurrently \"pnpm p:core watch\" \"pnpm p:rtv watch\" \"pnpm p:rt watch\" \"pnpm p:ts watch\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "pnpm": "10.8.1"
  },
  "packageManager": "pnpm@10.8.1",
  "volta": {
    "node": "22.10.0",
    "pnpm": "10.8.1"
  }
}
